    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "gh-pages": "^3.2.3",
    "nodemon": "^2.0.14"
  }
